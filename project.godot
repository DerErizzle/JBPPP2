; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "Config",
"language": "GDScript",
"path": "res://scripts/Config.gd"
}, {
"base": "Control",
"class": "Main",
"language": "GDScript",
"path": "res://scripts/Main.gd"
}, {
"base": "Panel",
"class": "Mod",
"language": "GDScript",
"path": "res://scripts/Mod.gd"
}, {
"base": "Panel",
"class": "MsgBox",
"language": "GDScript",
"path": "res://scripts/MsgBox.gd"
}, {
"base": "Control",
"class": "Settings",
"language": "GDScript",
"path": "res://scripts/Settings.gd"
}, {
"base": "Reference",
"class": "ZipExtractor",
"language": "GDScript",
"path": "res://scripts/ZipExtractor.gd"
} ]
_global_script_class_icons={
"Config": "",
"Main": "",
"Mod": "",
"MsgBox": "",
"Settings": "",
"ZipExtractor": ""
}

[application]

config/name="JPPP"
run/main_scene="res://Main.tscn"
run/low_processor_mode=true
run/low_processor_mode_sleep_usec=8000
boot_splash/image="res://pictures/splash.png"
config/icon="res://pictures/jbppp.png"
config/windows_native_icon="res://pictures/jbppp.ico"

[autoload]

Manager="*res://scripts/Manager.gd"

[display]

window/size/width=1280
window/size/height=720
window/vsync/use_vsync=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
threads/thread_model=2
threads/thread_safe_bvh=true
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
